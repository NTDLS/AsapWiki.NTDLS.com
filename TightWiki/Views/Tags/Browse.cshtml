@{
    Layout = "/Views/Shared/_Layout.cshtml";
    var wikiContext = ViewData["WikiContext"] as TightWiki.WikiContextState;
}

<div class="card">
    <div class="card-header"><strong>Associated Tags</strong></div>
    <div class="card-body">@Html.Raw(wikiContext?.TagCloud)</div>
</div>

<div class="card">
    <div class="card-header"><strong>Associated Pages</strong></div>
    <div class="card-body">@Html.Raw(wikiContext?.Pages)</div>
</div>
