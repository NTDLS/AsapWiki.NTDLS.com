@model AsapWiki.Shared.Models.Attachments

<form method="post" enctype="multipart/form-data">

    <input type="file" name="ImageData" id="ImageData" onchange="fileCheck(this);" />
    <div class="form-group"><button type="submit" class="btn btn-primary rounded-0">Upload</button></div>
</form>

@foreach (var x in Model.Files)
{
    <div>@x.Name</div>
}

